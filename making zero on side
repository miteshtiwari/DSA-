public class rough {
    public static void main(String[] args) {


        int[] arr = {0,0,1,0,3,0,5,0,6};
        int[] temp = new int[arr.length];
        int count = 0;
        for (int i = 0; i < arr.length ; i++)
        {
            if(arr[i]!=0)
            {
                temp[count] = arr[i];
                count++;
            }

        }
        for (int k = 0; k< temp.length; k++)
        {
            System.out.println(temp[k]);
        }

    }
}
