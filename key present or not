public class arrays
{

    public static String contain(int[] arr , int key)
    {
        for(int i = 0;i< arr.length;i++)
        {
            if(arr[i]==key)
            {
                return ("key is there");
            }
        }
        return "key is not there";

    }

    public static void main(String[] args)
    {
        int [] ap = {1,2,3,35,56,56,64,6,6647,889,746534,43,644,76};
        System.out.println(contain(ap,664 ));
    }


}
