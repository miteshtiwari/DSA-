public class rough
{
    public static void main(String[] args)
    {
        int [] myarr1 = {3,4,1,2,2};
        int [] myarr2 = {1,2,1,3,1};
        for(int i=0;i< myarr1.length;i++)
        {
            int count = 0;
            for (int j = 0; j < myarr2.length ; j++)
            {
                if(myarr1[i]<=myarr2[j])
                {
                    count++;
                }
            }
            System.out.println(count);
        }
    }
}
